@model IEnumerable<GerenciamentoPIM.Models.Aluno>



<h2 class="text-center">Gerenciador de Tarefas UNIPIM</h2>

<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Disciplina)
            </td>
        </tr>

        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
        </tr>

    }
</table>




